<view class="bg-wrap">

<text class="title" style="margin-top: 100rpx; margin-bottom: 50rpx;">GTCSSA之附属社团</text>

<view>
  <view class="cu-list grid col-2 no-border" style="padding-top: 70rpx;">
     <view class="cu-item" wx:for="{{departmentList}}" wx:if="{{index < gridCol*4}}" wx:key="{{item.id}}">
        <view> 
          <image src="{{item.icon}}" bindtap="{{item.bindtap}}" style="width:110rpx; height:110rpx;"> </image>
        </view>
        <text>{{item.name}}</text>
        <view style="height:50rpx; "></view>
      </view>
    </view>
</view>

</view>
